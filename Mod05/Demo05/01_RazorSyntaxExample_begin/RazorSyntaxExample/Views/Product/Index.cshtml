
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    @foreach (KeyValuePair<string, int> productPrices in ViewBag.ProductPrices)
    {
    <div>
        Product name: @productPrices.Key<br />
        Product price including tax: @(productPrices.Value * 1.2)
    </div>
    }
</body>
</html>
